{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/auth/(.*)",
            "dest": "/auth/$1"
        },
        {
            "src": ".*",
            "dest": "server.js"
        }
    ]
}